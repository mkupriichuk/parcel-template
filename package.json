{
  "name": "parcel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html -p 3000",
    "clean": "rimraf ./dist",
    "lint": "stylelint src/styles/**/*.sass",
    "tinify": "node ./config/tinify.config.js",
    "purify": "node ./config/purify.config.js",
    "structure": "node ./config/structure.js",
    "spell": "yaspeller ./src/*.html --find-repeat-words --dictionary ./config/speller_dict.json",
    "build": "rimraf ./dist && parcel build src/*.html --public-url ./ --no-source-maps"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-modules": "^0.1.3",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^7.1.0",
    "lodash.escaperegexp": "^4.1.2",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^2.0.0",
    "purify-css": "^1.2.5",
    "replace-in-file": "^6.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "stylelint": "^13.5.0",
    "tinify": "^1.6.0-beta.2",
    "yaspeller": "^7.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "safari >= 7",
    "ie >= 9",
    "ios >= 6"
  ]
}
